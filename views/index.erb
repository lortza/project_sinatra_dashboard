<h1>Dashboard</h1>

<table class="table table-striped table-hover table-sm">
  <thead class="">
    <tr>
      <th>title</th>
      <th>company</th>
      <th>location</th>
      <th>description</th>
      <th>date_posted</th>
      <th>salary</th>
      <th>skills</th>
      <th>remote</th>
    </tr>
  </thead>
  <tbody>
    <% jobs.each do |job| %>
    <tr>
      <td><a href="<%= job.description_url %>" target="_blank" alt="<%= job.title %>" title="<%= job.title %>"><%= job.title %></a> (<%= job.job_id %>)</td>
      <td><%= job.company %> (<%= job.company_id %>)</td>
      <td><%= job.location %></td>
      <td><%= job.description %></td>
      <td><%= job.date_posted %></td>
      <td><%= job.salary %></td>
      <td><%= job.skills %></td>
      <td><%= job.remote %></td>
    </tr>
    <% end %>
  </tbody>
</table>

